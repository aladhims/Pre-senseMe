<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="win.aladhims.presensigeofence.fragment.ListNgajarKuFragment">



    <android.support.v7.widget.RecyclerView
        android:id="@+id/rv_list_ngajarku"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:listitem="@layout/list_ngajarku_item"/>

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/fab_tambah_ngajar_baru"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_image_edit"/>
</RelativeLayout>
